
A :: (a: s32) -> s32
{
    b: s32 = a + 2;
    ret a * b;
}

B :: (h: s32)
{
    ret C(h, h * 2);
}

C :: (a: s32, b: s32)
{
    ret (a - 1) * (b + 1);
}

